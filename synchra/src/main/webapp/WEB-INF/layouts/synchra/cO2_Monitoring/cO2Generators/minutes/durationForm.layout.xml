<?xml version="1.0" encoding="utf-8" ?>

<config:template-call
	xmlns:config="http://www.top-logic.com/ns/config/6.0"
	template="com.top_logic.element/form.template.xml"
>
	<arguments>
		<name key="dynamic.e67f3507-65b6-4e87-878d-c8ba4a1b050c">
			<en>Duration</en>
			<de>Dauer</de>
		</name>
		<model class="com.top_logic.model.search.providers.TransformLinkingByExpression"
			function="output -> if ($output == null, new(`kpi:Anzahl`, transient: true), $output.copy())"
			input="model(self())"
		/>
		<forms>
			<form type="kpi:Anzahl">
				<formDefinition>
					<field
						attribute="count"
						fullQualifiedName="kpi:Anzahl#count"
						type="tl.core:Integer"
					/>
				</formDefinition>
			</form>
		</forms>
	</arguments>
</config:template-call>