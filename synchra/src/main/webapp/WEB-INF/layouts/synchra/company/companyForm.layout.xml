<?xml version="1.0" encoding="utf-8" ?>

<config:template-call
	xmlns:config="http://www.top-logic.com/ns/config/6.0"
	template="com.top_logic.element/form.template.xml"
>
	<arguments defaultFor="syn:Company">
		<name key="dynamic.2d136857-e976-4f33-8cb0-3534e0e5afac">
			<en>Company</en>
			<de>Gesellschaft</de>
		</name>
		<buttons>
			<button id="ID_14743b64_0a02_42bd_af8b_7ec66ca9735d"
				class="com.top_logic.model.search.providers.CommandHandlerByExpression"
				image="css:bi bi-arrow-right-circle"
				operation="sel -> $sel.inCurrent()"
			>
				<resourceKey key="dynamic.19769bc1-3f7c-48d2-baa4-b4cbf5e39fff">
					<en>Show current version</en>
					<de>Aktuelle Version zeigen</de>
				</resourceKey>
				<executability>
					<rule-by-expression decision="model -> $model.revision() == currentRevision()?false:true"/>
				</executability>
				<postCreateActions>
					<setModel target="model(self())">
						<input/>
					</setModel>
				</postCreateActions>
			</button>
		</buttons>
	</arguments>
</config:template-call>