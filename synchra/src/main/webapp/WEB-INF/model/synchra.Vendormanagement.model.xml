<?xml version="1.0" encoding="utf-8" ?>

<model xmlns="http://www.top-logic.com/ns/dynamic-types/6.0">
	<module name="synchra.Vendormanagement">
		<class name="Ansprechpartner">
			<generalizations>
				<generalization type="Contacts:PersonContact"/>
			</generalizations>
		</class>
		<class name="Vendor">
			<generalizations>
				<generalization type="Contacts:CompanyContact"/>
			</generalizations>
			<annotations>
				<form-definition>
					<form>
						<title>
							<label key="dynamic.5a02ce86-4a6f-4df9-bc4d-2deb0637a0f0">
								<de>Verwaltung eines Lieferanten</de>
							</label>
						</title>
						<field
							attribute="name"
							fullQualifiedName="Contacts:Contact.all#name"
							type="tl.core:String"
						/>
						<field
							attribute="remarks"
							fullQualifiedName="Contacts:Contact.all#remarks"
							type="tl.core:String"
						/>
						<group>
							<field
								attribute="country"
								fullQualifiedName="Contacts:Contact.Company#country"
								type="tl.util:Country"
							/>
							<field
								attribute="ZIPCode"
								fullQualifiedName="Contacts:Contact.Company#ZIPCode"
								type="tl.core:String"
							/>
							<field
								attribute="city"
								fullQualifiedName="Contacts:Contact.Company#city"
								type="tl.core:String"
							/>
							<field
								attribute="street"
								fullQualifiedName="Contacts:Contact.Company#street"
								type="tl.core:String"
							/>
							<field
								attribute="phone"
								fullQualifiedName="Contacts:Contact.all#phone"
								type="tl.core:String"
							/>
							<label key="dynamic.c17b53f7-f478-4063-9c7f-8dd88fad0c3d">
								<de>Adress- und Telefondaten</de>
							</label>
						</group>
						<field
							attribute="providerIsAbleToDeliver"
							fullQualifiedName="synchra.Vendormanagement:Vendor#providerIsAbleToDeliver"
							type="synchra.Bom:Component"
						/>
						<field
							attribute="lieferanthatAnsprechpartner"
							fullQualifiedName="synchra.Vendormanagement:Vendor#lieferanthatAnsprechpartner"
							type="synchra.Vendormanagement:Ansprechpartner"
						/>
					</form>
				</form-definition>
			</annotations>
			<attributes>
				<reference name="providerIsAbleToDeliver"
					inverse-reference="componentMayBeDeliveredBy"
					kind="forwards"
					multiple="true"
					navigate="true"
					type="synchra.Bom:Component"
				/>
				<reference name="lieferanthatAnsprechpartner"
					composite="true"
					kind="forwards"
					multiple="true"
					navigate="true"
					type="Ansprechpartner"
				/>
			</attributes>
		</class>
	</module>
</model>